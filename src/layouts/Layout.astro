---
import { AppInit } from '../components/AppInit'
import Rumble from '../services/rumble.astro'
const { title } = Astro.props
---

<html lang='en'>
  <head>
    <meta charset='UTF-8' />
    <meta name='viewport' content='width=device-width, initial-scale=1.0' />
    <title>{title}</title>
  </head>
  <body>
    <main>
      <Rumble />
      <AppInit client:only='react' />
      <slot />
    </main>
  </body>
</html>
